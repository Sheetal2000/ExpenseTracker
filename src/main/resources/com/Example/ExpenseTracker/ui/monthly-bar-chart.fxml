<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.Example.ExpenseTracker.ui.MonthlyBarChartController"
            prefHeight="400" prefWidth="600">

    <BarChart fx:id="barChart" layoutX="25" layoutY="25" prefHeight="350" prefWidth="550">
        <xAxis>
            <CategoryAxis fx:id="xAxis" label="Month"/>
        </xAxis>
        <yAxis>
            <NumberAxis fx:id="yAxis" label="Total Expenses"/>
        </yAxis>
    </BarChart>
</AnchorPane>
